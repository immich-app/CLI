{
  "name": "immich",
  "version": "0.30.0",
  "description": "Immich CLI Utilities",
  "author": "Alex Tran",
  "bin": {
    "immich": "./bin/run"
  },
  "homepage": "https://github.com/immich-app/CLI",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "immich-app/CLI",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.0.7",
    "@oclif/plugin-help": "^5.2.1",
    "@oclif/plugin-plugins": "^2.2.4",
    "axios": "^1.3.2",
    "chokidar": "^3.5.3",
    "fdir": "^5.3.0",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "rxjs": "^7.8.0",
    "systeminformation": "^5.17.8"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.3",
    "@types/chai": "^4.3.4",
    "@types/chokidar": "^2.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.11",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "immich-sdk": "^1.62.0",
    "mocha": "^9.2.2",
    "oclif": "^3.6.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "yaml": "^2.3.1"
  },
  "oclif": {
    "bin": "immich",
    "dirname": "immich",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/immich-app/CLI/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
