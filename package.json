{
  "name": "immich",
  "version": "0.30.0",
  "description": "Immich CLI Utilities",
  "author": "Alex Tran",
  "bin": {
    "immich": "./bin/run"
  },
  "homepage": "https://github.com/immich-app/CLI",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "immich-app/CLI",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.8.7",
    "@oclif/plugin-help": "^5.2.10",
    "@oclif/plugin-plugins": "^3.1.5",
    "axios": "^1.4.0",
    "chokidar": "^3.5.3",
    "fdir": "^6.0.1",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "rxjs": "^7.8.1",
    "systeminformation": "^5.18.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.25",
    "@types/chai": "^4.3.5",
    "@types/chokidar": "^2.1.3",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^10.0.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "immich-sdk": "^1.62.0",
    "jest": "^29.5.0",
    "jest-message-util": "^29.5.0",
    "jest-when": "^3.5.2",
    "mocha": "^10.2.0",
    "mock-fs": "^5.2.0",
    "oclif": "^3.9.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "yaml": "^2.3.1"
  },
  "oclif": {
    "bin": "immich",
    "dirname": "immich",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/immich-app/CLI/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
